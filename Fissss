-- ƒê·∫£m b·∫£o r·∫±ng b·∫°n ƒë√£ c√≥ m·ªôt GUI ƒë·ªÉ hi·ªÉn th·ªã
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- T·∫°o Frame (Menu) ƒë·ªÉ hi·ªÉn th·ªã bi·ªÉu t∆∞·ª£ng m√®o
local menuFrame = Instance.new("Frame")
menuFrame.Size = UDim2.new(0, 200, 0, 200)
menuFrame.Position = UDim2.new(0.5, -100, 0.5, -100)
menuFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
menuFrame.Parent = ScreenGui

-- T·∫°o TextLabel ƒë·ªÉ hi·ªÉn th·ªã bi·ªÉu t∆∞·ª£ng m√®o
local catIcon = Instance.new("TextLabel")
catIcon.Text = "üê±"  -- Bi·ªÉu t∆∞·ª£ng m√®o
catIcon.Font = Enum.Font.SourceSans
catIcon.TextSize = 100
catIcon.TextColor3 = Color3.fromRGB(0, 0, 0)  -- M√†u ƒëen cho bi·ªÉu t∆∞·ª£ng
catIcon.Size = UDim2.new(1, 0, 1, 0)  -- L√†m cho TextLabel chi·∫øm to√†n b·ªô frame
catIcon.Parent = menuFrame

-- H√†m m·ªü menu
local function openMenu()
    menuFrame.Visible = true  -- Khi menu m·ªü, s·∫Ω hi·ªÉn th·ªã
end

-- H√†m ƒë√≥ng menu
local function closeMenu()
    menuFrame.Visible = false  -- Khi ƒë√≥ng menu, ·∫©n menu ƒëi
end

-- M·ªü menu khi game b·∫Øt ƒë·∫ßu
openMenu()

-- ƒê√≥ng menu sau 5 gi√¢y
wait(5)
closeMenu()
